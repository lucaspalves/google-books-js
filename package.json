{
  "name": "google-books-js",
  "author": "Lucas Alves",
  "version": "1.0.1",
  "description": "A Google Books API wrapper for using in your Javascript projects.",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "scripts": {
    "test": "jest",
    "start": "nodemon src/index.ts",
    "build": "rm -rf dist && tsc --project tsconfig.production.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "keywords": [
    "google-books",
    "google",
    "books",
    "api",
    "js",
    "javascript",
    "node",
    "react",
    "vue",
    "angular"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-jest": "^29.3.1",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^27.0.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "files": [
    "dist/**/*"
  ]
}
